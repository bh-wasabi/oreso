{{#define id="notaIncidente"}}  
  {{#transform id="copiar"}}
    {{#update section="base"}}
      {{set solicitud="=_id"}}
      {{set esIncidente="=true"}}
      {{set tipoIncidente="=base.tipoIncidente"}}
      {{set reporte="=base.reporte"}}
      {{set cliente="=base.cliente"}}
      {{set proyecto="=base.proyecto"}}
      {{set inmueble="=base.inmueble"}}
      {{set zona="=base.zona"}}
      {{set ubicacion="=base.ubicacion"}}
    {{/update}}
  {{/transform}}
  {{action id="notaAsignacion" source="notaAsignacion" label="Asignaci√≥n" transform="copiar" hide="true" type="add-subdoc" visibleMode="close" color="deep-orange" btnSolid="true" onlyPost="true"}}
{{/define}}

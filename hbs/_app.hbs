  {{#view id="persona"}}
    {{#find}}
      {{include field="_name"}}
      {{include field="_created"}}
      {{include field="persona.nombreCompleto"}}
      {{include field="persona.clave"}}
      {{include field="persona.genero"}}
      {{include field="persona.fechaNacimiento"}}
      {{include field="persona.tipoSanguineo"}}
      {{sort field="_id" direction="desc"}}
      {{search field="_name"}}
      {{search field="persona.nombreCompleto"}}
      {{search field="persona.clave"}}
      {{search field="persona.fechaNacimiento"}}
    {{/find}}
  {{/view}}
  {{#view id="lista-asunto"}}
    {{#find}}
      {{include field="_created"}}
      {{include field="base.categoria"}}
      {{include field="base.subCategoria"}}
      {{include field="base.fechaOcurrio"}}
      {{include field="base.descripcion"}}
      {{include field="base.estatus"}}
      {{sort field="base.estatus" direction="asc"}}
      {{sort field="_id" direction="desc"}}
      {{search field="_name"}}
      {{search field="base.descripcion"}}
      {{search field="base._categoria"}}
      {{search field="base._subCategoria"}}
    {{/find}}
    {{calc field="estaPendiente" type="expr" value="base.estatus==='abierto'"}}
  {{/view}}


      <li class="has-action-left">        
        <span style="font-size:12px;float:right;">{{date base.fechaOcurrio "DD/MMM/YYYY h:mm a"}}</span>
        <div class="list-action-left"><i class="icon {{#if estaPendiente}}ion-alert text-red{{else}}ion-checkmark text-grey{{/if}}"></i></div>
        <div class="list-content">
          <span class="title">{{_name}}{{#if estaPendiente}} ({{fromNow base.fechaOcurrio}}){{/if}}</span>
          <span style="font-size:12px;float:right;">{{_created._user}}</span>
          <div style="font-size:12px;">
            <span>{{base._subCategoria}}</span><br>
            <span>
              {{base.descripcion}}                
            </span>
          </div>
        </div>
      </li>
    </div>
  {{/template}}    

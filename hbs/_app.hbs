  {{#template id="persona"}}
    <div>
      {{_name}}</span>
      <span style="font-size:12px;float:right;">{{date _created.date "DD/MMM/YYYY h:mm a"}}</span><br>
      {{#if persona}}
      <span style="font-size:12px;">{{persona.nombreCompleto}}</span>
      <span style="font-size:12px;float:right;">{{persona._genero}}, {{persona._tipoSanguineo}}</span>
      {{/if}}
